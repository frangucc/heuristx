<%= javascript_tag do -%>
$(document).ready(function(){
  var rand = $('.slideshow-three > div').length;
  $('.slideshow-three').cycle({
    pager: '#home_slide_nav3',
    cleartype: 1,
    timeout:0,
    startingSlide: Math.floor(Math.random() * rand),
    fx: 'none'
  });
  $("#connect_and_exchange_wrapper").css({opacity: 0, visibility: "visible"});
  var html = $(".hidden-media-widget").html();
  $(".hidden-media-widget").remove();
  $("#connect_and_exchange_wrapper").html(html);
  $("#connect_and_exchange_wrapper").css({opacity: 0, visibility: "visible"}).animate({opacity: 1}, 1000);
  
  $(".mast-text-dillyeo").hide();
  $(".mast-text-dillyeo").slideDown("slow");
});
function loadVideo1() {
  jQuery.facebox('<iframe src="http://player.vimeo.com/video/7880283?portrait=0" width="650" height="440" frameborder="0"></iframe><br><br>');
  $(".fb-content").trigger("update");
}
function loadVideo2() {
  jQuery.facebox('<iframe src="http://www.190north.com/shows/120708_cyber.htm" width="450" height="320" scrolling="No" frameborder="0"></iframe><br><br>');
  $(".fb-content").trigger("update");
}
function loadVideo3() {
  jQuery.facebox('<object width="640" height="385"><param name="movie" value="http://www.youtube.com/v/vWoiT7vVEIU?fs=1&amp;hl=en_US"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="http://www.youtube.com/v/vWoiT7vVEIU?fs=1&amp;hl=en_US" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="640" height="385"></embed></object>');
  $(".fb-content").trigger("update");
}
function loadVideo4() {
  jQuery.facebox('<iframe src="/stupidMe.html" width="650" height="440" frameborder="0"></iframe><br><br>');
  $(".fb-content").trigger("update");
}
<% end -%>

